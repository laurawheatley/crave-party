  {
    "name": "Crave Party",
    "version": "1.0",
    "description": "Stream Crave in sync with your buddies",
    "permissions": [
      "storage",
      "contextMenus",
      "activeTab",
      "declarativeContent"
    ],
    "background": {
      "scripts": ["thirdParty/jquery-3.5.0.js", "src/background.js"],
      "persistent": true
    },
    "page_action": {
      "default_icon": {                   
        "16": "images/logo.png",
        "24": "images/logo.png",
        "32": "images/logo.png"
      },
      "default_title": "Crave Party"
    },
    "content_scripts": [{
      "css": ["src/styles.css"],
      "js": ["thirdParty/jquery-3.5.0.js", "src/partyScript.js"],
      "html": ["src/script.html"],
      "matches": ["https://www.crave.ca/*"]
    }],
    "manifest_version": 2
  }